{
  "name": "mini-ttl",
  "version": "0.0.0",
  "description": "Miniature temporary value creator",
  "main": "source/index.js",
  "scripts": {
    "format": "prettier --write '{source,test}/**/*.js'",
    "precommit": "lint-staged",
    "test": "npm run test:unit && npm run test:format",
    "test:format": "prettier-check '{source,test}/**/*.js'",
    "test:unit": "mocha -r test/index.js 'test/unit/**/*.spec.js'"
  },
  "lint-staged": {
    "{source,test}/**/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/perry-mitchell/mini-ttl.git"
  },
  "keywords": [
    "ttl",
    "expiry",
    "temporary",
    "value",
    "store"
  ],
  "author": "Perry Mitchell <perry@perrymitchell.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/perry-mitchell/mini-ttl/issues"
  },
  "homepage": "https://github.com/perry-mitchell/mini-ttl#readme",
  "dependencies": {
    "eventemitter3": "^3.1.0",
    "ms": "^2.1.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "mocha": "^5.2.0",
    "prettier": "^1.14.2",
    "prettier-check": "^2.0.0",
    "sinon": "^6.1.5",
    "sleep-promise": "^8.0.1"
  }
}
